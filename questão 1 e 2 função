#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int soma(int a,int b){
	int res;
	res = a + b;
	return res;
}
int subtrai(int a,int b,int c){
	int res;
	res= a- b- c;
	return res;
}
int main() {
	int num1,num2,num3,resut1,resut2;
	scanf("%d", &num1);
	scanf("%d", &num2);
	scanf("%d", &num3);
	resut1=soma(num1,num2);
	resut2=subtrai(num1,num2,num3);
	printf("%d\n",resut1);
	printf("%d",resut2);
	return 0;
}
